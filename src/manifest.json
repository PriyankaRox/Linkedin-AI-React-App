{
  "manifest_version": 3,
  "name": "LinkedIn AI Reply",
  "version": "1.0",
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/*"],
      "js": ["contentScript.js"],
      "css": ["contentStyles.css"]
    }
  ],
  "action": {
    "default_popup": "index.html"
  }
}
